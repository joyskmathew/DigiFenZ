{
	"name": "DigiFenZ",
	"version": "1.0",
	"description": "Remote Isolation Browser Extension For Chrome!",
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "DigiFenZ",
		"default_icon": {
			"16": "images/logo16.png",
			"32": "images/logo32.png",
			"48": "images/logo48.png",
			"128": "images/logo128.png"
		}
	},
	"permissions": ["contextMenus"],
	"background": {
		"scripts": ["popup.js"]
	},
	"icons": {
		"16": "images/logo16.png",
		"32": "images/logo32.png",
		"48": "images/logo48.png",
		"128": "images/logo128.png"
	},
	"manifest_version": 2
}
